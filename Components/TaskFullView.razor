

<MudDialog>
    <DialogContent>
        <MudPaper Width="100%">
            <MudContainer MaxWidth="MaxWidth.Medium">
                <MudText Typo="Typo.caption">@model.TaskState</MudText>
                <MudText Typo="Typo.subtitle1">@model.TaskDescription</MudText>
            </MudContainer>
        </MudPaper>
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Close">Cerrar</MudButton>
    </DialogActions>
</MudDialog>

@code{
    [CascadingParameter] 
    MudDialogInstance MudDialog { get; set; }

    [Parameter, EditorRequired]
    public Taskes? model {get;set;}

      private void Close()
    {
        MudDialog.Close(DialogResult.Ok(true));
    }
}